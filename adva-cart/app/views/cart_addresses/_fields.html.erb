<%= f.label :email %>
<%= f.text_field :email %>

<%= f.fields_for(:shipping_address) do |a| %>
	<%= a.label :name %>
	<%= a.text_field :name %>

	<%= a.label :street %>
	<%= a.text_field :street %>

	<%= a.label :zipcode %>
	<%= a.text_field :zipcode %>

	<%= a.label :city %>
	<%= a.text_field :city %>

	<%= a.label :country %>
	<%= a.text_field :country %>
<% end %>

<%= f.label :delivery_method %>
<%= f.select :delivery_method, options_for_select(Adva::Cart::DELIVERY_METHODS) %>
